#include <stdio.h> 
int main() 
{ int n,m,i,j; 
printf("Enter No of elements in A: "); 
scanf("%d",&n);
 printf("Enter No of elements in B: "); 
scanf("%d",&m); 
int a[n],b[m],f=0;
printf("Enter elements of set A:\n");
 for(i=0;i<n;i++) 
{ 
scanf("%d",&a[i]); 
} 
printf("Enter number of elements in B:\n");
for(i=0;i<m;i++)
{ 
    scanf("%d",&b[i]);
 }
 printf("Difference of sets :\n");
 for(i=0;i<n;i++) 
{
    f=0; for(j=0;j<m;j++)
 { 
     if(a[i]==b[j])
 { 
     f=f+1;
 break; 
} 
} if(f==0)
 { 
     printf("\t%d",a[i]); 
} 
} 
return 0; 
}
